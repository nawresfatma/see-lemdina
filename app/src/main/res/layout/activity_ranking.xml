            app:layout_constraintEnd_toEndOf="@+id/FirstplaceName"
            app:layout_constraintStart_toStartOf="@+id/FirstplaceName"
            app:layout_constraintTop_toBottomOf="@+id/FirstplaceName" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rankingRecycler"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView54"
            tools:listitem="@layout/ranki_tem" />


