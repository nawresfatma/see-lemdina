            app:layout_constraintEnd_toEndOf="@+id/FirstplaceImg"
            app:layout_constraintHorizontal_bias="0.517"
            app:layout_constraintStart_toStartOf="@+id/FirstplaceImg"
            app:layout_constraintTop_toBottomOf="@+id/FirstplaceImg" />

